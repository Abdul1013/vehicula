  bk_repository                    bk_repository                      @relation(fields: [v_s_entry_by], references: [bk_uid], onDelete: NoAction, map: "vehicle_subscriptions_history_ibfk_3")
  vehicle_sub_plan_heading         vehicle_sub_plan_heading           @relation(fields: [v_s_plan_id_fk], references: [s_p_heading_id], onDelete: NoAction, map: "vehicle_subscriptions_history_ibfk_4")

  @@unique([v_s_plate_id_fk, v_s_chart_id_fk, v_s_expiry_date], map: "v_s_plate_id_fk_2")
  @@index([v_s_chart_id_fk], map: "v_s_chart_id_fk")
  @@index([v_s_entry_by], map: "v_s_entry_by")
  @@index([v_s_plan_id_fk], map: "v_s_plan_id_fk")
  @@index([v_s_plate_id_fk], map: "v_s_plate_id_fk")
}

model vehicle_surplus_pay {
  v_sr_id                String                 @id @db.VarChar(20)
  v_sr_plate_id_fk       BigInt
  v_sr_amount            Decimal                @db.Decimal(10, 2)
  v_sr_amount_paid       Decimal                @default(0.00) @db.Decimal(10, 2)
  v_sr_entry_by          BigInt
  v_sr_entry_on          DateTime               @db.DateTime(0)
  vehicule_plate_numbers vehicule_plate_numbers @relation(fields: [v_sr_plate_id_fk], references: [v_plate_id], onDelete: NoAction, map: "vehicle_surplus_pay_ibfk_1")

  @@index([v_sr_plate_id_fk], map: "v_sr_plate_id_fk")
}

model vehicle_type {
  v_type_id              Int                      @id @default(autoincrement())
  v_type_name            String                   @unique(map: "v_type_name") @db.VarChar(250)
  v_type_order           Int                      @db.TinyInt
  v_type_icon            String                   @db.VarChar(50)
  v_type_img             String?                  @db.VarChar(1000)
  vehicle_price_chart    vehicle_price_chart[]
  vehicule_plate_numbers vehicule_plate_numbers[]
}

model vehiculars_otp_req_log {
  otp_log_id    BigInt        @id @default(autoincrement())
  otp_uid_fk    BigInt
  otp_code      String        @db.VarChar(50)
  otp_date      DateTime      @db.DateTime(0)
  bk_repository bk_repository @relation(fields: [otp_uid_fk], references: [bk_uid], onDelete: Cascade, map: "vehiculars_otp_req_log_ibfk_1")

  @@index([otp_uid_fk], map: "otp_uid_fk")
}

model vehicule_plate_numbers {
  v_plate_id                    BigInt                          @id @default(autoincrement())
  v_plate_no                    String                          @unique(map: "v_plate_no") @db.VarChar(50)
  v_person_id                   BigInt
  v_type_fk                     Int
  v_cat_fk                      Int
  v_plate_status                Int                             @default(1) @db.TinyInt
  v_plate_entry_date            DateTime                        @db.DateTime(0)
  vehicle_subscriptions         vehicle_subscriptions[]
  vehicle_subscriptions_history vehicle_subscriptions_history[]
  vehicle_surplus_pay           vehicle_surplus_pay[]
  bk_repository                 bk_repository                   @relation(fields: [v_person_id], references: [bk_uid], onDelete: NoAction, map: "vehicule_plate_numbers_ibfk_1")
  vehicle_categories            vehicle_categories              @relation(fields: [v_cat_fk], references: [v_cat_id], onDelete: NoAction, map: "vehicule_plate_numbers_ibfk_2")
  vehicle_type                  vehicle_type                    @relation(fields: [v_type_fk], references: [v_type_id], onDelete: NoAction, map: "vehicule_plate_numbers_ibfk_3")

  @@index([v_cat_fk], map: "v_cat_fk")
  @@index([v_person_id], map: "v_person_id")
  @@index([v_type_fk], map: "v_type_fk")
}
